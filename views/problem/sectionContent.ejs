<% switch(sc) { %>
<% case description: %>
	<%= problem.description %>
	<% break; %>
<% case statement: %>
	<%= problem.statement %>
	<% break; %>
<% case examples: %>
	<%= problem.examples %>
	<% break; %>
<% case importance: %>
	<%= problem.importance %>
	<% break; %>
<% case hardness: %>
	<%= problem.hardness %>
	<% break; %>
<% case history: %>
	<%= problem.history %>
	<% break; %>
<% case solutions: %>
	<%= problem.solutions %>
	<% break; %>
<% case implications: %>
	<%= problem.implications %>
	<% break; %>
<% case faq: %>
	<%= problem.faq %>
	<% break; %>
<% case forums: %>
	<%= problem.forums %>
	<% break; %>
<% case references: %>
	<%= problem.references %>
	<% break; %>
<% default: break; %>
<% } %>