
	<section id="examples">
		<h3>Examples</h3>
		<hr>
		<!-- from http://getbootstrap.com/2.3.2/javascript.html#tabs -->
		<ul class="nav nav-tabs">
			<li class="active"><a href="#examplesView" data-toggle="tab">View</a></li>
			<li><a href="#examplesEdit" data-toggle="tab">Edit</a></li>
		</ul>
		<div id="examplesContent" class="tab-content">
			<div class="tab-pane fade in active" id="examplesView">
			    <p>
					<%= problem.examples %>
			    </p>
			</div>
			<div class="tab-pane fade" id="examplesEdit">
			<form name="examplesInput" action="/submitEdit">
				<input type="hidden" name="problemName" value="<%= problem.nid %>">
				<input type="hidden" name="problemSection" value="examples">
				<input type="hidden" name="problemOldText" value="<%= problem.examples %>">
			    	<textarea name="problemNewText" rows="10" cols="300" class="span9"><%= problem.examples %></textarea>
				<button type="submit" class="btn">Save</button>
			</form>
			</div>
		</div>
	</section>